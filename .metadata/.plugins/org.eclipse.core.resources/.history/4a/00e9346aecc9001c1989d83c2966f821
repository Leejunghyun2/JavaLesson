package chap08;

public class MyExceptionTest {
	public static void main(String[] args) {
		try {
		method();
	} catch(MyException e) {
		System.out.println(e.getErrCode());
		System.out.println(e.getMessage());
	}
		
	}
	
	static void method() throws MyException {
		throw new MyException("나의오류");
	}
}
