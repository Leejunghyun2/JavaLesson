package NewPersonExam01;

public class Menu {

	int  color;
	int gb, pr;
	String jo,g1, g2;
	int price;

	Menu(String jo, int color, int gb ,int price) {

		this.jo = jo;
		this.color = color;
		this.gb = gb;
		
	}

	Menu() {

	}

	void showMenu() {
		
		System.out.printf("종류 : %s \n색상 : %s \n용량 : %d GB \n가격 : %d원", jo, g2, pr,price);

	}
	
		
	

}
